{
  "name": "@trinkets/core",
  "version": "0.1.0",
  "tasks": {
    "test": "deno test -A --unstable-kv",
    "test:unit": "deno test -A src/domain_test.ts src/result_test.ts",
    "test:integration": "deno test -A --unstable-kv src/integration_test.ts",
    "test:e2e": "deno test -A --unstable-kv src/e2e_test.ts",
    "test:concurrency": "deno test -A src/concurrency_test.ts",
    "test:production": "deno test -A --unstable-kv src/production_test.ts src/performance_test.ts",
    "test:all": "deno test -A --unstable-kv src/*_test.ts",
    "fmt": "deno fmt",
    "lint": "deno lint",
    "publish": "jsr publish",
    "tr": "deno run -A cli/tr.ts",
    "init-merge": "deno run -A cli/init_merge.ts"
  },
  "exports": {
    ".": "./src/index.ts",
    "./domain": "./src/domain.ts",
    "./query": "./src/query.ts",
    "./search": "./src/search.ts",
    "./embed": "./src/embed.ts",
    "./ports": "./src/ports.ts",
    "./result": "./src/result.ts",
    "./stores/jsonl": "./src/store_jsonl.ts",
    "./stores/heads": "./src/store_jsonl_heads_v2.ts",
    "./cache/kv": "./src/cache_kv.ts",
    "./cache/sqlite": "./src/cache_sqlite.ts",
    "./indexed": "./src/indexed_graph.ts",
    "./query/indexed": "./src/query_indexed.ts",
    "./observability": "./src/observability.ts",
    "./retry": "./src/retry.ts",
    "./integrity": "./src/integrity.ts",
    "./backup": "./src/backup.ts",
    "./schemas": "./src/schemas_runtime.ts"
  },
  "compilerOptions": {
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "lib": ["ES2022", "deno.ns", "deno.window"]
  }
}
