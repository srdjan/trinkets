{
  "name": "@trinkets/core",
  "version": "0.1.0",
  "tasks": {
    "test": "deno test -A --fail-fast",
    "fmt": "deno fmt",
    "lint": "deno lint",
    "publish": "jsr publish",
    "tr": "deno run -A cli/tr.ts",
    "init-merge": "deno run -A cli/init_merge.ts"
  },
  "exports": {
    ".": "./src/index.ts"
  },
  "compilerOptions": {
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "lib": ["ES2022", "deno.ns", "deno.window"]
  }
}
